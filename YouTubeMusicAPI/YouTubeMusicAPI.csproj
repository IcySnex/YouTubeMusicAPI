<Project Sdk="Microsoft.NET.Sdk">
	
	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<LangVersion>12</LangVersion>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>

		<Version>3.0.2</Version>
		<AssemblyVersion>3.0.2</AssemblyVersion>
		<FileVersion>3.0.2</FileVersion>

		<Title>YouTube Music API</Title>
		<Authors>IcySnex</Authors>
		<Company />
		<Description>YouTube Music Web API Wrapper for C#</Description>
		<PackageProjectUrl>https://github.com/IcySnex/YouTubeMusicAPI</PackageProjectUrl>
		<PackageIcon>icon.png</PackageIcon>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<RepositoryUrl>https://github.com/IcySnex/YouTubeMusicAPI/tree/main</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>youtube;youtubemusic;music</PackageTags>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<PackageReleaseNotes>
			- Fixed getting streaming data when authenticated (sig deciphering extraction)
			- Fixed general search (without SearchCategory) always being empty
			- Fixed missing log events in RequestHelper
			- Updated NuGet packages
		</PackageReleaseNotes>
	</PropertyGroup>
	
	<ItemGroup>
	  <None Include="..\icon.png">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	  <None Include="..\LICENSE">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	  <None Include="..\README.md">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>
	
	<ItemGroup>
	  <PackageReference Include="Jint" Version="4.4.1" />
	  <PackageReference Include="Microsoft.Bcl.AsyncInterfaces" Version="9.0.10" />
	  <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="9.0.10" />
	  <PackageReference Include="Newtonsoft.Json" Version="13.0.4" />
	</ItemGroup>
	
</Project>